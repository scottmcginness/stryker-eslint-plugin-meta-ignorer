{
  "name": "stryker-eslint-plugin-meta-ignorer",
  "version": "1.0.0",
  "description": "Stryker ignorer for the meta property of ESLint plugin rules",
  "license": "MIT",
  "author": "Scott McGinness",
  "keywords": [
    "stryker",
    "ignorer",
    "mutation",
    "eslint",
    "plugin"
  ],
  "main": "src/index.mjs",
  "scripts": {
    "format:pkg": "format-package -w",
    "lint": "eslint",
    "stryker": "npx stryker run",
    "test:generate": "node --test test/generator.test.mjs",
    "test:integration": "npm run stryker && npm run test:integration-post-generate",
    "test:integration-post-generate": "node --test test/integration.test.mjs"
  },
  "devDependencies": {
    "@eslint/js": "^9.4.0",
    "@stryker-mutator/api": "^8.2.6",
    "@stryker-mutator/core": "^8.2.6",
    "@types/node": "^20.14.0",
    "dedent": "^1.5.3",
    "fake-diff": "^1.0.0",
    "format-package": "^7.0.0",
    "globals": "^15.3.0",
    "prettier": "3.3.0"
  }
}
